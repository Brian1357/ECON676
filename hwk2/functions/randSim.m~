function [MVP_list,TP_list] = randSim(stocks, mean_r,mean_rf,cov_mat,timerange,rep)
%randSim:
%r_mean:
%cov_mat: covariance matrix
%timerange: number of data in the given time period
%rep: repetition


MVP_list = zeros(2,rep);
TP_list =zeros(2,rep);
cov_mat = cov(stocks)
for i = 1:rep
    new_stocks = mvnrnd(mean_r,cov_mat,timerange);
    cov_mat1 = cov(new_stocks);
    mean_r1 = (mean(new_stocks))';
    
    [MVP,TP] = MVTP(mean_r1,mean_rf,cov_mat1);
    MVP_list(:,i) = MVP(3:end)'*mean_r;
    TP_list(:,i) = TP(3:end)'*mean_r;
end
end